development:
  sessions:
    default:
      hosts:
        - localhost:27017
      database: techcor_development
      logger: false
  options:
    raise_not_found_error: false

test:
  sessions:
    default:
      hosts:
        - localhost:27017
      database: techcor_test
      logger: false
  options:
    raise_not_found_error: false

production:
  sessions:
    default:
      uri: mongodb://localhost:27017/techcor_production
      logger: false
  options:
    raise_not_found_error: false
